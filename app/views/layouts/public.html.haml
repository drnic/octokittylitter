    %script{ :type => "text/javascript" }
      github_user = null
    = javascript_include_tag :all
    = yield(:head)
  #_firebugConsole{ :FirebugVersion => "1.4.3b1", :style => "display: none;" }
  %body
    #main
      #header.basic
        .site
          .logo
            %a{ :href => "/" }
              %img{ :src => "/images/logov3.png", :alt => "github" }
          .topsearch
            %form#top_search_form{ :action => "/search", :method => "get" }
              %a.advanced-search{ :href => "/search", :title => "Advanced Search" }
