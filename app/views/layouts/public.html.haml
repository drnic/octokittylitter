              %input{ :name => "type", :type => "hidden", :value => "Everything" }
              %input{ :name => "repo", :type => "hidden", :value => "" }
              %input{ :name => "langOverride", :type => "hidden", :value => "" }
              %input{ :name => "start_value", :type => "hidden", :value => "1" }
            %ul.nav.logged_out
              %li
                %a{ :href => "/" }
                  Home
              %li.pricing
                %a{ :href => "/plans" }
                  Pricing and Signup
              %li
                %a{ :href => "/popular/forked" }
                  Explore GitHub
              %li
                %a{ :href => "/blog" }
                  Blog
              %li
                %a{ :href => "/login" }
                  Login
      .site
        = yield
      .push
